    "studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "drizzle-orm": "^0.38.3",
    "pg": "^8.13.1",
    "postgres": "^3.4.5",
    "@dependify/config": "workspace:*"
  },
  "devDependencies": {
    "drizzle-kit": "^0.30.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "@types/pg": "^8.11.10"
  }
